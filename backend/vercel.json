{
	"version": 2,
	"builds": [
		{
			"src": "*.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1",
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE"
			],
			"headers": {
				"Access-Control-Allow-Origin": "https://sport-app-eosin.vercel.app, http://localhost:3000",
				"Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
				"Access-Control-Allow-Headers": "Content-Type, Authorization"
			}
		},
		{
			"src": "/(.*)",
			"dest": "/"
		}
	]
}